<!DOCTYPE unspecified PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
{% block slyle %}
<link href="{{ asset('assets/css/bootstrap.min.css') }}" type="text/css"
	rel="stylesheet">
<link href="{{ asset('assets/css/jquery.datetimepicker.css') }}"
	type="text/css" rel="stylesheet">
<link href="{{ asset('assets/css/wallet.css') }}" type="text/css"
	rel="stylesheet">

<script type="text/javascript"
	src="{{ asset('assets/js/jquery-2.1.3.min.js') }}"></script>
<script type="text/javascript"
	src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
<script type="text/javascript"
	src="{{ asset('assets/js/jquery.datetimepicker.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/js/wallet.js') }}"></script>

<link rel="icon" type="image/x-icon" href="/favicon.ico">
<meta name="viewport" content="width=device-width, initial-scale=1">

{% endblock %}
<title>{% block title %}Денюжки семейные{% endblock %}</title>
</head>

<body>

	<nav class="navbar navbar-default">
	<div class="container-fluid">
		<!-- Brand and toggle get grouped for better mobile display -->
		<div class="navbar-header">
			<button type="button" class="navbar-toggle collapsed"
				data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
				<span class="sr-only">Toggle navigation</span> <span
					class="icon-bar"></span> <span class="icon-bar"></span> <span
					class="icon-bar"></span>
			</button>
			<a class="navbar-brand" href="{{ path('app_wallet_wallet') }}">AlMoney</a>
		</div>

		<!-- Collect the nav links, forms, and other content for toggling -->
		<div class="collapse navbar-collapse"
			id="bs-example-navbar-collapse-1">
			<ul class="nav navbar-nav">
				{% block main_links %}
				<li {% if app.request.get('_route') == 'app_wallet_wallet_1' %}
					class="active" {% endif %}><a
					href="{{ path('app_wallet_wallet_1') }}">Кошелек<span
						class="sr-only">(current)</span></a></li>
				<li {% if app.request.get('_route') == 'app_wallet_status' %}
					class="active" {% endif %}><a
					href="{{ path('app_wallet_status') }}">Состояние</a></li>
				<li>
				<li
					{% if app.request.get('_route') == 'almo_wallet_analytics_index' %}
					class="active" {% endif %}><a
					href="{{ path('app_analytics_index') }}">Аналитика</a></li>
				<li><a href="#">Настройки</a></li>
				{% endblock %}
			</ul>
			<ul class="nav navbar-nav navbar-right">
				<li><a href="{{ path('app_security_logout') }}">Выход</a></li>
			</ul>
		</div>
		<!-- /.navbar-collapse -->
	</div>
	<!-- /.container-fluid --> </nav>

	<div class="container {% block al_container_class %}{% endblock%}">
		{% block submenu %}
		{% endblock %}

		{% block messages %}
		{% if error is defined and error %}
		<div class="alert alert-danger" role="alert">
			<span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
			<span class="sr-only">Error:</span>
			{{ error.message }}
		</div>
		{% endif %}
		{% endblock %}

		{% block body %}

		{% endblock %}
	</div>
	<!-- /.container -->

</body>

</html>
